{
	"name": "obsidian-linkeddata-plugin",
	"version": "1.0.0",
	"description": "This is a LinkedData plugin for Obsidian (https://obsidian.md)",
	"main": "src/main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"dist": "rm -rf dist && mkdir dist && npm run build && cp styles.css versions.json manifest.json dist/",
		"release": "node version-bump.mjs",
		"test": "jest --passWithNoTests"
	},
	"keywords": [
		"obsidian",
		"linked data"
	],
	"author": "Thomas Woelfle",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/react": "^18.2.24",
		"@types/react-dom": "^18.2.8",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@types/winston": "^2.4.4",
		"rdflib": "^2.2.32",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"winston": "^3.11.0"
	}
}
